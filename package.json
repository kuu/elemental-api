{
  "name": "elemental-api",
  "version": "0.0.3",
  "description": "Node client library and CLI to access REST APIs on AWS Elemental appliances & software",
  "main": "./lib/index.js",
  "bin": {
    "ele": "./cli/index.js"
  },
  "scripts": {
    "lint": "xo",
    "ele": "node ./cli/index.js",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "audit": "npm audit --audit-level high",
    "test": "npm run lint && npm run audit && ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kuu/elemental-api.git"
  },
  "keywords": [],
  "author": "Kuu Miyazaki <miyazaqui@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kuu/elemental-api/issues"
  },
  "homepage": "https://github.com/kuu/elemental-api#readme",
  "dependencies": {
    "config": "^3.2.2",
    "debug": "^4.1.1",
    "minimist": "^1.2.0",
    "node-fetch": "^2.6.0",
    "through2-parallel": "^0.1.3"
  },
  "devDependencies": {
    "ava": "^2.2.0",
    "coveralls": "^3.0.6",
    "nyc": "^14.1.1",
    "rewire": "^4.0.1",
    "sinon": "^7.4.1",
    "xo": "^0.24.0"
  },
  "ava": {
    "helpers": [
      "test/helper/**/*"
    ]
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "camelcase": 0,
      "capitalized-comments": 0,
      "new-cap": 0,
      "no-await-in-loop": 0,
      "no-multi-assign": 0,
      "no-return-assign": 0,
      "padding-line-between-statements": 0,
      "import/no-dynamic-require": 0,
      "unicorn/catch-error-name": 0
    }
  }
}
